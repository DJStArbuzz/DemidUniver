#include <iostream>

using namespace std;


int main()
{
    int n;
    cin >> n;
    for (int i = 0; i < n; i++) {
        int n;
        int a1, a2, a3, a4;
        cin >> a1 >> a2;

        a3 = a1 ^ a2;
        a4 = a2 ^ a3;

        cin >> n;

        n %= 3;
        if (n == 0) {
            cout << a1 << "\n";
        }
        if (n == 1) {
            cout << a2 << "\n";
        }

        if (n == 2) {
            cout << a3 << "\n";
        }


    }
}


#include <iostream>
#include <vector>
#include <string>
#include <set>
#include <algorithm>
#include <cmath>

using namespace std;

int main() {
	int t;
	cin >> t;
	for (int test = 0; test < t; test++) {
		int a, b, c;
		cin >> a >> b >> c;
		if (a != b && b == c) {
			cout << a << endl;
		}
		else if (b != a && a == c) {
			cout << b << endl;
		}
		else {
			cout << c << endl;
		}
	}

	return 0;
}

/*
Вам даны три цифры a
, b
, c
. Две из них равны, но третья отличается от остальных двух.

Найдите значение, которое встречается ровно один раз.*/

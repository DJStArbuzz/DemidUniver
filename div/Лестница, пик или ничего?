#include <iostream>
#include <vector>
#include <map>
#include <random>
#include <algorithm>
#include <string>

using namespace std;

int main()
{
	int t;
	cin >> t;

	for (int test = 0; test < t; test++) {
		int a, b, c;
		cin >> a >> b >> c;

		if (a < b && b < c) {
			cout << "STAIR\n";
		}
		else if (a < b && b > c) {
			cout << "PEAK\n";
		}
		else {
			cout << "NONE\n";
		}
	}
}

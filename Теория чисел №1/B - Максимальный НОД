#include <iostream>
#include <vector>
#include <map>
#include <random>
#include <algorithm>
#include <string>

using namespace std;

int main()
{
    int t;
    cin >> t;

    for (int test = 0; test < t; test++) {
        int n, res = 1;
        cin >> n;

        if (n % 2 == 0) {
            cout << n / 2 << '\n';
        }
        else {
            cout << (n - 1) / 2 << '\n';
        }
    }

	return 0;

}

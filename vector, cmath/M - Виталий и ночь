#include <iostream>
#include <cmath>
#include <vector>
#include <string>
#include <algorithm>

using namespace std;

int main()
{
    int n, m, res = 0;
    cin >> n >> m;

    for (int i = 0; i < n * m; i++) {
        int tmp1, tmp2;
        cin >> tmp1 >> tmp2;
        res += (tmp1 || tmp2);
    }
                
    cout << res;

    return 0;

}
